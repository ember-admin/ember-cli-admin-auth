{{#each action in controller.breadcrumbsActions}}
  {{#if action}}
    <div class="breadcrumb-action">
      {{#view 'admin/base/actions/action' tagName="button" href="#" breadcrumbActionBinding="action" controllerBinding="controller"}}
        {{view.title}}
      {{/view}}
    </div>
  {{/if}}
{{/each}}
